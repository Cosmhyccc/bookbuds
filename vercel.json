{
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [{ "key": "Content-Type", "value": "application/json" }]
    },
    {
      "source": "/.well-known/apple-app-site-association/",
      "headers": [{ "key": "Content-Type", "value": "application/json" }]
    }
  ],
  "rewrites": [
    { "source": "/profile/:path*", "destination": "/fallback/index.html" },
    { "source": "/bonfire/:path*", "destination": "/fallback/index.html" }
  ]
}
